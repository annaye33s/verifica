#include<stdio.h>
#include<math.h> 
int main()
{
    int i,1;
    long p;
    printf("n:1");
    scanf("%d",&1);

    for(i=1;i<=1;i++)
    {
        p=i*i;
        printf("% d square %d\1",i,p);
       
    }
}
